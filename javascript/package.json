{
    "name": "citekit",
    "version": "0.1.6",
    "description": "Local-first AI resource mapping SDK for multimodal agents open specific parts of files instead of sending entire documents.",
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "default": "./dist/index.js"
        }
    },
    "scripts": {
        "build": "tsc",
        "dev": "tsc --watch"
    },
    "dependencies": {
        "@google/generative-ai": "^0.24.1",
        "@modelcontextprotocol/sdk": "^1.0.0",
        "zod": "^3.23.0",
        "commander": "^12.0.0"
    },
    "peerDependencies": {
        "fluent-ffmpeg": "^2.1.3",
        "pdf-lib": "^1.17.1",
        "sharp": "^0.34.5"
    },
    "peerDependenciesMeta": {
        "fluent-ffmpeg": {
            "optional": true
        },
        "pdf-lib": {
            "optional": true
        },
        "sharp": {
            "optional": true
        }
    },
    "bin": {
        "citekit": "./dist/cli.js"
    },
    "devDependencies": {
        "@types/fluent-ffmpeg": "^2.1.28",
        "@types/node": "^20.0.0",
        "fluent-ffmpeg": "^2.1.3",
        "pdf-lib": "^1.17.1",
        "sharp": "^0.34.5",
        "typescript": "^5.4.0",
        "vitepress": "^1.6.4",
        "vue": "^3.5.28"
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/abdushakurob/citekit.git",
        "directory": "javascript"
    },
    "keywords": [
        "ai",
        "sdk",
        "llm",
        "mcp",
        "document",
        "video"
    ],
    "author": "",
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "directories": {
        "example": "examples"
    },
    "bugs": {
        "url": "https://github.com/abdushakurob/citekit/issues"
    },
    "homepage": "https://github.com/abdushakurob/citekit#readme"
}